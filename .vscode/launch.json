{
  "version": "0.2.0",
  "compounds": [
    {
      "name": "(Windows) Launch frogrs (2 instances)",
      "configurations": [
        "(Windows) Launch frogrs_game",
        "(Windows) Launch frogrs_game (other)"
      ],
      "preLaunchTask": "rust: cargo build"
    }
  ],
  "configurations": [
    {
      "cwd": "${workspaceRoot}",
      "args": [
        "--port",
        "7000",
        "--players",
        "local",
        "--players",
        "127.0.0.1:7001"
      ],
      "name": "(Windows) Launch frogrs_game",
      "type": "cppvsdbg",
      "request": "launch",
      "console": "internalConsole",
      "program": "${workspaceRoot}/target/debug/frogrs_game.exe",
      "stopAtEntry": false,
      "environment": [
        { "name": "RUST_BACKTRACE", "value": "0" },
        { "name": "CARGO_MANIFEST_DIR", "value": "${workspaceFolder}" }
      ],
      "preLaunchTask": "rust: cargo build"
    },
    {
      "cwd": "${workspaceRoot}",
      "args": [
        "--port",
        "7001",
        "--players",
        "local",
        "--players",
        "127.0.0.1:7000"
      ],
      "name": "(Windows) Launch frogrs_game (other)",
      "type": "cppvsdbg",
      "request": "launch",
      "console": "internalConsole",
      "program": "${workspaceRoot}/target/debug/frogrs_game.exe",
      "stopAtEntry": false,
      "environment": [
        { "name": "RUST_BACKTRACE", "value": "0" },
        { "name": "CARGO_MANIFEST_DIR", "value": "${workspaceFolder}" }
      ],
      "preLaunchTask": "rust: cargo build"
    },
    {
      "cwd": "${workspaceRoot}",
      "args": ["--players", "4", "--check-distance", "7"],
      "name": "(Windows) Launch frogrs_synctest",
      "type": "cppvsdbg",
      "request": "launch",
      "console": "internalConsole",
      "program": "${workspaceRoot}/target/debug/frogrs_synctest.exe",
      "stopAtEntry": false,
      "environment": [
        { "name": "RUST_BACKTRACE", "value": "0" },
        { "name": "CARGO_MANIFEST_DIR", "value": "${workspaceFolder}" }
      ],
      "preLaunchTask": "rust: cargo build"
    }
  ]
}
