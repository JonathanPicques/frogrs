{
  "version": "0.2.0",
  "configurations": [
    {
      "cwd": "${workspaceRoot}",
      "args": [
        "--port",
        "6001",
        "--players",
        "local",
        "--players",
        "127.0.0.1:6002"
      ],
      "name": "(Windows) Launch frogrs_game",
      "type": "cppvsdbg",
      "request": "launch",
      "console": "internalConsole",
      "program": "${workspaceRoot}/target/debug/frogrs_game.exe",
      "stopAtEntry": false,
      "environment": [
        { "name": "RUST_BACKTRACE", "value": "0" },
        { "name": "CARGO_MANIFEST_DIR", "value": "${workspaceFolder}" }
      ],
      "preLaunchTask": "Rust: cargo build - frogrs"
    },
    {
      "cwd": "${workspaceRoot}",
      "args": ["--players", "2", "--check-distance", "2"],
      "name": "(Windows) Launch frogrs_synctest",
      "type": "cppvsdbg",
      "request": "launch",
      "console": "internalConsole",
      "program": "${workspaceRoot}/target/debug/frogrs_synctest.exe",
      "stopAtEntry": false,
      "environment": [
        { "name": "RUST_BACKTRACE", "value": "0" },
        { "name": "CARGO_MANIFEST_DIR", "value": "${workspaceFolder}" }
      ],
      "preLaunchTask": "Rust: cargo build - frogrs"
    }
  ]
}
